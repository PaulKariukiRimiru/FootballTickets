<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="myk.project.FootballTickets">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="myk.project.Activities.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="myk.project.Activities.WelcomeActivity"
            android:label="@string/title_activity_welcome"
            android:theme="@style/AppTheme.NoActionBar" >

        </activity>
        <activity
            android:name="myk.project.Activities.CartActivity"
            android:label="@string/title_activity_cart"
            android:parentActivityName="myk.project.Activities.HomeActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mike.footballtickets.myk.Activities.HomeActivity" />
        </activity>
        <activity
            android:name="myk.project.Activities.Account"
            android:label="@string/title_activity_account"
            android:parentActivityName="myk.project.Activities.HomeActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mike.footballtickets.myk.Activities.HomeActivity" />
        </activity>
        <activity
            android:name="myk.project.Activities.QrScannerAcitvity"
            android:label="@string/title_activity_qr_scanner_acitvity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="myk.project.Activities.Payments" />
        <activity android:name="myk.project.Activities.AboutActivity" />
        <activity
            android:name="myk.project.Activities.TeamAccount"
            android:label="@string/title_activity_team_account"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="myk.project.Activities.TeamMainActivity"
            android:label="@string/title_activity_team_main"
            android:theme="@style/AppTheme.NoActionBar" >

        </activity>
        <activity
            android:name="myk.project.Activities.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name="myk.project.Activities.WalletActivity"
            android:label="@string/title_activity_wallet"
            android:parentActivityName="myk.project.Activities.HomeActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mike.footballtickets.myk.Activities.HomeActivity" />
        </activity>
    </application>

</manifest>